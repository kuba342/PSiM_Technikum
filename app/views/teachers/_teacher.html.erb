<div id="<%= dom_id teacher %>">
  <p>
    <strong>Firstname:</strong>
    <%= teacher.firstName %>
  </p>

  <p>
    <strong>Lastname:</strong>
    <%= teacher.lastName %>
  </p>

  <p>
    <strong>User:</strong>
    <br>
    <%= "Id: " + teacher.user_id.to_s %>
    <br>
    <%= "Login: " + teacher.user.login %>
  </p>

</div>
