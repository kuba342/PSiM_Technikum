<div id="<%= dom_id student %>">
  <p>
    <strong>Firstname:</strong>
    <%= student.firstName %>
  </p>

  <p>
    <strong>Lastname:</strong>
    <%= student.lastName %>
  </p>

  <p>
    <strong>User:</strong>
    <br>
    <%= "Id: " + student.user_id.to_s %>
    <br>
    <%= "Login: " + student.user.login %>
  </p>

  <p>
    <strong>Class of student:</strong>
    <%= student.class_of_student.symbol %>
  </p>

</div>
