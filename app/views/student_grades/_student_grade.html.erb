<div id="<%= dom_id student_grade %>">
  <p>
    <strong>Grade:</strong>
    <%= student_grade.grade %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= student_grade.date %>
  </p>

  <p>
    <strong>Teacher:</strong>
    <%= student_grade.teacher.name %>
  </p>

  <p>
    <strong>Student:</strong>
    <%= student_grade.student.student_infor %>
  </p>

  <p>
    <strong>Course:</strong>
    <%= student_grade.course.name %>
  </p>

  <p>
    <strong>Test:</strong>
    <%= student_grade.test.test_infor %>
  </p>

</div>
